{
  "name": "agrohub",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail --prefix name --names \"BACKEND,FRONTEND\" --prefix-colors \"yellow,cyan\" \"cd backend && npm start\" \"vite\"",
    "dev-db": "concurrently --kill-others-on-fail --prefix name --names \"BACKEND,FRONTEND\" --prefix-colors \"yellow,cyan\" \"cd backend && node server.js\" \"vite\"",
    "start": "node start.js",
    "frontend": "vite",
    "backend": "cd backend && node server.js",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "chart.js": "^4.5.0",
    "gh-pages": "^6.3.0",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.10",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.0.4"
  }
}
